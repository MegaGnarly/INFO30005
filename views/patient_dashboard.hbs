        <h1 class="heading">Dashboard</h1>
        <br>

        <!-- Grid container that stores profile data and 'buttons' (e.g. record health)-->
        <div id="grid-container">
            <!-- Column 1-->
            <div id="profile">
                <img src="assets/icon_profile.png" alt="Profile picture" width="175" height="175">
                <h2 class="username">John Doe</h2>
                <p class="ranking">Ranking: 3rd</p>
            </div>

            <!-- Column 2 -->
            <!-- Grid container that stores buttons-->
            <div id="grid-container-nested">
                <button class="element">Record Health</button>
                <button class="element purple1">View History</button>
                <button class="element purple2">Leaderboard</button>
                <button class="element purple3">Settings</button>

            </div>
        </div>
        <br>
        <h1 class="heading">Notifications</h1>
        <div id="grid-container">
            <!-- Column 1-->
            <div id="profile"></div>

            <!-- Column 2 -->
            <!-- Grid container that stores buttons-->
            <div id="grid-container-nested">
                <button class="element white">Dose of insulin</button>

                <button class="element-msg">
                    <h2>New message!</h2>
                    <p>Great job!</p>
                </button>
                <button class="element white">Blood glucose levels</button>
            </div>
        </div>
